  @import url('https://fonts.googleapis.com/css2?family=Source+Sans+3:ital,wght@0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap');
  * {
  font-family: 'Source Sans 3', sans-serif;
  }
  html {
  min-height: 100%;
  display: flex;
  background: radial-gradient(circle, #212121 1%, #000000 70%);
  }
  body {
  font-size: 0.9em;
  padding: 0;
  margin: 0;
  justify-content: center;
  min-height: 100%;
  flex: 1;
  display: flex;
  flex-direction: column;
  font-family: 'Source Sans 3', sans-serif;
  color: #fff;

  animation: parallaxmove 160s linear infinite;
  }


  @keyframes parallaxmove {
  0% {
    background-position: 0% bottom;
  }
  100% {
    background-position: calc(100vw * -3) bottom, calc(100vw * -2) bottom;
  }
  }

  .signupbtn {
  background: linear-gradient(180deg, rgba(184,0,255,1) 0%, rgba(106,0,147,1) 100%); 
  border: 1px solid #470062; 
  color: #fff; 
  text-align: center; 
  flex: 1; 
  width: 100%; 
  font-size: 1.75em; 
  box-sizing: border-box; 
  padding: 0.2em 0; 
  font-weight: 500;
  cursor: pointer;
  }

  .signupbtn:hover {
  background: rgb(189,20,255);
  background: linear-gradient(180deg, rgba(189,20,255,1) 0%, rgba(143,0,199,1) 100%);
  }
  

  .container {
  margin: 0 auto;
  padding: 0.4em;
  box-sizing: border-box;
  width: 100%;
  max-width: 850px;
  }

  a.link:link {
  color: #fff;
  text-decoration: none;
}
a.link:visited {
  color: #fff;
}
a.link:hover {
  color: #fff;
  text-decoration: underline;
}
a.link:active {
  color: #fff;
}
